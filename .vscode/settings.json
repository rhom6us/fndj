{

  "vscode-edge-devtools.defaultUrl": "http://localhost:9080/",
  "explorerExclude.backup": {},
  "vscode-edge-devtools.pathMapping": {
  
    // "webpack:///./*": "${webRoot}/*",
    // "webpack:///src/*": "${webRoot}/*",
    // "webpack:///*": "*",
    // "webpack:///./~/*": "${webRoot}/node_modules/*",
    "webpack://*": "${webRoot}/packages/*"
  },
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/Thumbs.db": false,
    "**/lib": false,
    "**/.rush": false,
    "packages/@fndj/browser/public": false,
    "**/temp": false,
    "**/dist": false
  },
  "explorer.fileNesting.patterns": {
    "*.cs": "${capture}.*.cs",
    "*.tsx": "${capture}.stories.tsx",
  },
}
