{

  "vscode-edge-devtools.defaultUrl": "http://localhost:9080/",
  "explorerExclude.backup": {},
  "vscode-edge-devtools.pathMapping": {
  
    // "webpack:///./*": "${webRoot}/*",
    // "webpack:///src/*": "${webRoot}/*",
    // "webpack:///*": "*",
    // "webpack:///./~/*": "${webRoot}/node_modules/*",
    "webpack://*": "${webRoot}/packages/*"
  },
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "files.eol": "\n",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/lib": true,
    "**/.rush": true,
    "packages/@fndj/browser/public": true,
    "**/temp": true,
    "**/dist": true
  },
  "explorer.excludeGitIgnore": true,
}
