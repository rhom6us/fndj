{
  "search.exclude": {
    "**/.yarn": false,
    "**/.pnp.*": false
  },
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "source.fixAll.eslint": true,
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/*.csproj.user": false,
    "**/bin/": false,
    "**/obj/": false,
    "**/Properties/Resources.resx": false,
    "yarn.lock": false,
    "**/yarn-error.log": false,
    "packages/@fndj/webpack/lib": false,
    "packages/@fndj/webpack/__tests__": false,
    "dist": false,
    "packages/**/node_modules/[!@]*": false,
    "**/*.d.ts.map": false,
    ".*": false,
    "**/desktop.ini": false,
    "**/*.rar": false,
    "packages/YouTubeProxy": false,
    "packages/@types": false,
    "packages/@fndj/webpack": false,
    "**/node_modules": false,
    "pnpm-lock.yaml": false,
    "packages\\@fndj/superpowered": false,
    "packages/fniterate": false,
    "packages/redux-command-pattern": false
  },

  // The package manager you use to install node modules.
  "prettier.packageManager": "yarn",

  // The package manager you use to install node modules.
  "tslint.packageManager": "yarn",

  // The package manager you use to install node modules.
  "eslint.packageManager": "pnpm",
  "editor.formatOnSaveMode": "modifications",
  "editor.formatOnType": true,
  "editor.formatOnSave": true,
  "typescript.format.semicolons": "insert",
  "eslint.format.enable": true,
  "explorerExclude.backup": null,
  "typescript.tsdk": "node_modules\\typescript\\lib",
  "npm.packageManager": "pnpm",
  "files.autoSaveDelay": 3000,
  "files.autoSave": "off",
  "vscode-edge-devtools.browserFlavor": "Beta",
  "vscode-edge-devtools.defaultUrl": "http://localhost:9080/",
  "vscode-edge-devtools.headless": true

}
